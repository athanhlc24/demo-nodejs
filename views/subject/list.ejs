<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
<div class="container" style="margin-top: 10px">
    <div><a href="#">Thêm mới</a></div>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Tên Môn</th>
            <th scope="col">Số giờ học</th>
            <th scope="col">Tên_Giáo viên</th>
            <th scope="col">Email_Giáo viên</th>
            <th scope="col">Điện thoại_Giáo viên</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
        </tr>
        </thead>
        <tbody>
        <% items.map((item,key)=>{%>
            <tr>
                <th scope="row"><%= key +1 %></th>
                <td><%= item.tenmon%></td>
                <td><%= item.sogio%></td>
                <td><%= item.giaovien.tengiaovien%></td>
                <td><%= item.giaovien.email%></td>
                <td><%= item.giaovien.dienthoai%></td>
                <td><a href="/subjects/edit/<%= item._id %>">Edit</a></td>
                <form action="/subjects/delete/<%= item._id%>" method="post">
                    <td><button  onclick="return confirm('Bạn có chắc chắn xoá <%= item.tenmon%>')" type="submit">Delete</button></td>
                </form>
            </tr>
        <%})%>

        </tbody>
    </table>
</div>

</body>
</html>